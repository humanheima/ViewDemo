<?xml version="1.0" encoding="utf-8"?>
<com.hm.viewdemo.cancel_event.InterceptTouchEventFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context=".cancel_event.TestCancelEventActivity">


    <com.hm.viewdemo.cancel_event.MyButton
        android:id="@+id/btn"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:background="@color/colorAccent" />

    <com.hm.viewdemo.widget.MyImageView
        android:id="@+id/ivImage"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_gravity="end"
        android:background="@color/black" />


</com.hm.viewdemo.cancel_event.InterceptTouchEventFrameLayout>